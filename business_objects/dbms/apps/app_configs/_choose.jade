table.ui.compact.striped.selectable.celled.table.choose-rows
    thead
        th App Configs
        th Code
        th Config
        th Valor
        th Fields
        th Observações
    tbody
        tr.cursor(
            rv-each-row='choose.data.rows'
            rv-on-click='choose.select'
            rv-data-key='row.app_configs_key'
        )
            td {row.app_configs_key}
            td {row.code}
            td {row.config}
            td {row.valor}
            td {row.fields}
            td {row.observacoes}