.ui.fluid.card(rv-each-row='dbms.apps.app_logs.data.rows')
    .content
        .header {row.app_logs_key} - {row.log}
        .meta Keys: app_logs_tipos_key, sec_usuarios_key, empresas_key
        .description

    .extra.content
        span.left.floated.button
            button.ui.orange.icon.mini.button(data-action='dbms apps app_logs edit', rv-data-key='row.app_logs_key')
                i.edit.icon
                | Editar

        span.right.floated.button
            button.ui.red.icon.mini.button(rv-data-action='dbms apps app_logs delete', rv-data-key='row.app_logs_key')
                i.delete.icon
                | Remover
