.ui.fluid.card(rv-each-row='dbms.enderecos.[object Object].data.rows')
    .content
        .header {row.enderecos_key} - {row.endereco}
        .meta Keys: end_bairros_key, end_cidades_key, end_estados_key, end_paises_key
        .description

    .extra.content
        span.left.floated.button
            button.ui.orange.icon.mini.button(data-action='dbms enderecos [object Object] edit', rv-data-key='row.enderecos_key')
                i.edit.icon
                | Editar

        span.right.floated.button
            button.ui.red.icon.mini.button(rv-data-action='dbms enderecos [object Object] delete', rv-data-key='row.enderecos_key')
                i.delete.icon
                | Remover
