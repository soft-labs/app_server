.ui.fluid.card(rv-each-row='dbms.movimentacoes.movimentacoes.data.rows')
    .content
        .header {row.movimentacoes_key} - {row.competencia}
        .meta Keys: mov_tipos_key, mov_origem_key, mov_destino_key, mov_status_key, empresas_key, parceiros_key, emp_clientes_key, emp_fornecedores_key, contratos_key, emp_dep_origem_key, emp_dep_destino_key
        .description

    .extra.content
        span.left.floated.button
            button.ui.orange.icon.mini.button(data-action='dbms movimentacoes movimentacoes edit', rv-data-key='row.movimentacoes_key')
                i.edit.icon
                | Editar

        span.right.floated.button
            button.ui.red.icon.mini.button(rv-data-action='dbms movimentacoes movimentacoes delete', rv-data-key='row.movimentacoes_key')
                i.delete.icon
                | Remover
