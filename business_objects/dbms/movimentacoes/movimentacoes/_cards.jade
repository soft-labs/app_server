    .extra.content
        span.left.floated.button
            button.ui.orange.icon.mini.button(data-action='dbms movimentacoes [object Object] edit', rv-data-key='row.movimentacoes_key')
                i.edit.icon
                | Editar

        span.right.floated.button
            button.ui.red.icon.mini.button(rv-data-action='dbms movimentacoes [object Object] delete', rv-data-key='row.movimentacoes_key')
                i.delete.icon
                | Remover
