table.ui.compact.striped.selectable.celled.table.choose-rows
    thead
        th Parceiros
        th Rh Cargos
        th Dt Inicio
        th Dt Enceramento
        th Função
        th Observações
    tbody
        tr.cursor(
            rv-each-row='choose.data.rows'
            rv-on-click='choose.select'
            rv-data-key='row.parceiros_key'
        )
            td {row.parceiros_key}
            td {row.rh_cargos_key}
            td {row.dt_inicio}
            td {row.dt_enceramento}
            td {row.funcao}
            td {row.observacoes}