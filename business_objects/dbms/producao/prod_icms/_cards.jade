.ui.fluid.card(rv-each-row='dbms.producao.[object Object].data.rows')
    .content
        .header {row.prod_icms_key} - {row.icms}
        .meta Keys: prod_cod_tributacao_key
        .description

    .extra.content
        span.left.floated.button
            button.ui.orange.icon.mini.button(data-action='dbms producao [object Object] edit', rv-data-key='row.prod_icms_key')
                i.edit.icon
                | Editar

        span.right.floated.button
            button.ui.red.icon.mini.button(rv-data-action='dbms producao [object Object] delete', rv-data-key='row.prod_icms_key')
                i.delete.icon
                | Remover
