table.ui.compact.striped.selectable.celled.table.choose-rows
    thead
        th Item Lotes
        th Dt Lote
        th Dt Valid De
        th Dt Valid Ate
        th Lote
        th Lote Id De
        th Lote Id Ate
        th Qtd Lote
        th Observações
    tbody
        tr.cursor(
            rv-each-row='choose.data.rows'
            rv-on-click='choose.select'
            rv-data-key='row.item_lotes_key'
        )
            td {row.item_lotes_key}
            td {row.dt_lote}
            td {row.dt_valid_de}
            td {row.dt_valid_ate}
            td {row.lote}
            td {row.lote_id_de}
            td {row.lote_id_ate}
            td {row.qtd_lote}
            td {row.observacoes}