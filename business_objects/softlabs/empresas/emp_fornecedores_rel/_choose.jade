table.ui.compact.striped.selectable.celled.table.choose-rows
    thead
        th Empresas
        th Emp Fornecedores
        th Ativo
        th Dt Inicio
        th Ultima Compra
        th Classificação
        th Observações
    tbody
        tr.cursor(
            rv-each-row='choose.data.rows'
            rv-on-click='choose.select'
            rv-data-key='row.empresas_key'
        )
            td {row.empresas_key}
            td {row.emp_fornecedores_key}
            td {row.ativo}
            td {row.dt_inicio}
            td {row.ultima_compra}
            td {row.classificacao}
            td {row.observacoes}