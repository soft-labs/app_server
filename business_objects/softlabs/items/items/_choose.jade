table.ui.compact.striped.selectable.celled.table.choose-rows
    thead
        th Items
        th Item Categorias
        th Item Categ Sub
        th Cont Centros Resultado
        th Ativo
        th Codigo
        th Item
        th Observações
    tbody
        tr.cursor(
            rv-each-row='choose.data.rows'
            rv-on-click='choose.select'
            rv-data-key='row.items_key'
        )
            td {row.items_key}
            td {row.item_categorias_key}
            td {row.item_categ_sub_key}
            td {row.cont_centros_resultado_key}
            td {row.ativo}
            td {row.codigo}
            td {row.item}
            td {row.observacoes}