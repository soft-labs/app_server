.ui.fluid.card(rv-each-row='softlabs.contabil.[object Object].data.rows')
    .content
        .header {row.cont_diario_resultado_key} - {row.historico}
        .meta Keys: cont_centro_resultados_key, empresas_key, movimentacoes_key, fin_lancamentos_key, fin_baixas_key
        .description

    .extra.content
        span.left.floated.button
            button.ui.orange.icon.mini.button(data-action='softlabs contabil [object Object] edit', rv-data-key='row.cont_diario_resultado_key')
                i.edit.icon
                | Editar

        span.right.floated.button
            button.ui.red.icon.mini.button(rv-data-action='softlabs contabil [object Object] delete', rv-data-key='row.cont_diario_resultado_key')
                i.delete.icon
                | Remover
