table.ui.compact.striped.selectable.celled.table.choose-rows
    thead
        th App Integrações
        th App Integ Config
        th Sec Usuarios
        th Dt Integração
        th Observações
    tbody
        tr.cursor(
            rv-each-row='choose.data.rows'
            rv-on-click='choose.select'
            rv-data-key='row.app_integracoes_key'
        )
            td {row.app_integracoes_key}
            td {row.app_integ_config_key}
            td {row.sec_usuarios_key}
            td {row.dt_integracao}
            td {row.observacoes}