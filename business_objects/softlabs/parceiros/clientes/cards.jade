.ui.fluid.card(rv-each-row='data.rows')
    .content
        .header {row.parceiros_key} - {row.parceiros_key}
        .meta Keys: end_paises_key, parc_contas_key, cont_plano_contas_key, cont_centro_resultados_key
        .description

    .extra.content
        span.left.floated.button
            button.ui.orange.icon.mini.button(rv-data-action='api.edit', rv-data-key='row.parceiros_key')
                i.edit.icon
                | Editar

        span.right.floated.button
            button.ui.red.icon.mini.button(rv-data-action='api.delete', rv-data-key='row.parceiros_key')
                i.delete.icon
                | Remover
