table.ui.compact.striped.selectable.celled.table.choose-rows
    thead
        th Parceiros
        th Razão Social
        th Cnpj
        th Insc Estadual
        th Insc Municipal
        th Fone Empresa
        th Email Empresa
        th Site Empresa
        th Observações
    tbody
        tr.cursor(
            rv-each-row='choose.data.rows'
            rv-on-click='choose.select'
            rv-data-key='row.parceiros_key'
        )
            td {row.parceiros_key}
            td {row.razao_social}
            td {row.cnpj}
            td {row.insc_estadual}
            td {row.insc_municipal}
            td {row.fone_empresa}
            td {row.email_empresa}
            td {row.site_empresa}
            td {row.observacoes}