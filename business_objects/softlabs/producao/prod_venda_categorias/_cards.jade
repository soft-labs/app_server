.ui.fluid.card(rv-each-row='softlabs.producao.[object Object].data.rows')
    .content
        .header {row.prod_venda_categorias_key} - {row.prod_venda_categorias_key}
        .meta Keys: cont_plano_contas_key
        .description

    .extra.content
        span.left.floated.button
            button.ui.orange.icon.mini.button(data-action='softlabs producao [object Object] edit', rv-data-key='row.prod_venda_categorias_key')
                i.edit.icon
                | Editar

        span.right.floated.button
            button.ui.red.icon.mini.button(rv-data-action='softlabs producao [object Object] delete', rv-data-key='row.prod_venda_categorias_key')
                i.delete.icon
                | Remover
