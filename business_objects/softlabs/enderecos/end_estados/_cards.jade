.ui.fluid.card(rv-each-row='softlabs.enderecos.[object Object].data.rows')
    .content
        .header {row.end_estados_key} - {row.estado}
        .meta Keys: end_paises_key
        .description

    .extra.content
        span.left.floated.button
            button.ui.orange.icon.mini.button(data-action='softlabs enderecos [object Object] edit', rv-data-key='row.end_estados_key')
                i.edit.icon
                | Editar

        span.right.floated.button
            button.ui.red.icon.mini.button(rv-data-action='softlabs enderecos [object Object] delete', rv-data-key='row.end_estados_key')
                i.delete.icon
                | Remover
