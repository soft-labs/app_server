.ui.fluid.card(rv-each-row='softlabs.contratos.[object Object].data.rows')
    .content
        .header {row.contr_item_locacao_key} - {row.contr_item_locacao_key}
        .meta Keys: item_identificado_key, contr_items_key
        .description

    .extra.content
        span.left.floated.button
            button.ui.orange.icon.mini.button(data-action='softlabs contratos [object Object] edit', rv-data-key='row.contr_item_locacao_key')
                i.edit.icon
                | Editar

        span.right.floated.button
            button.ui.red.icon.mini.button(rv-data-action='softlabs contratos [object Object] delete', rv-data-key='row.contr_item_locacao_key')
                i.delete.icon
                | Remover
