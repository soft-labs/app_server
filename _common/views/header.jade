doctype html
html
    head
        block title
            title='App Server - TShark 3.0'

        meta(charset = 'utf-8')
        meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
        meta(name='viewport', content='width=device-width, initial-scale=1.0, maximum-scale=1.0')

        // JQuery
        script(type='text/javascript', src='js/3rd/jquery/jquery-2.1.1.min.js')

        // Alertify
        link(rel='stylesheet', href="js/3rd/alertifyjs/css/alertify.min.css")
        link(rel='stylesheet', href="js/3rd/alertifyjs/css/themes/default.min.css", id="toggleCSS")
        script(type="application/javascript", src="js/3rd/alertifyjs/alertify.min.js")

        // Semantic
        link(rel='stylesheet', href='js/3rd/semantic/dist/semantic.min.css')
        script(type='application/javascript', src='js/3rd/semantic/dist/semantic.js')

        // Rivet
        //script(type='text/javascript', src='js/3rd/rivets/rivets.min.js')
        script(type='text/javascript', src='js/3rd/rivets/sightglass.js')
        script(type='text/javascript', src='js/3rd/rivets/rivets.js')
        script(type='text/javascript', src='js/3rd/rivets/stdlib.js')

        // Chart
        script(type='text/javascript', src='js/3rd/chartjs/Chart.min.js')
        script(type='text/javascript', src='js/3rd/chartjs/Chart.bundle.min.js')

        // Momentjs
        //script(type='text/javascript', src='js/3rd/moment/moment-with-locales.min.js')

        // Pickadate
        link(rel='stylesheet', href="js/3rd/pickadatejs/lib/compressed/themes/default.css")
        link(rel='stylesheet', href="js/3rd/pickadatejs/lib/compressed/themes/default.date.css")
        link(rel='stylesheet', href="js/3rd/pickadatejs/lib/compressed/themes/default.time.css")
        script(type='text/javascript', src='js/3rd/pickadatejs/lib/compressed/picker.js')
        script(type='text/javascript', src='js/3rd/pickadatejs/lib/compressed/picker.date.js')
        script(type='text/javascript', src='js/3rd/pickadatejs/lib/compressed/picker.time.js')
        script(type='text/javascript', src='js/3rd/pickadatejs/lib/compressed/translations/pt_BR.js')

        // Tshark
        link(rel="stylesheet", href="client/v3/_engine.css")
        script(type="application/javascript", src="client/v3/_extends.js")
        script(type="application/javascript", src="client/v3/_rivets.js")
        script(type="application/javascript", src="client/v3/tshark.js")
        script(type="application/javascript", src="client/v3/_api.js")
        script(type="application/javascript", src="client/v3/_modulo.js")
        script(type="application/javascript", src="client/v3/_dataset.js")
        script(type="application/javascript", src="client/v3/_forms.js")

        // App
        link(rel='stylesheet', href='css/app.css')
        link(rel='stylesheet', href='css/responsive.css')
        script(type='application/javascript', src='index.js')

        // Estilos do app
        block app_css

        // Javascripts do app
        block app_scripts

    body
        block content
